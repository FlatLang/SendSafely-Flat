{
  "name": "SendSafely-Flat",
  "version": "0.1.0",
  "description": "SendSafely Flat language Client API",
  "author": "Braden Steffaniak",
  "license": "ISC",
  "defaultTarget": "es6",
  "scripts": {
    "test": "node dist/es6/test.js",
    "create-package": "node dist/es6/create-package.js"
  },
  "sources": {
    "main": {
      "mainClass": "flat/sendsafely/SendSafely",
      "source": "src",
      "dependencies": {
        "https://github.com/FlatLang/Datastruct": "master",
        "https://github.com/FlatLang/Time": "master",
        "https://github.com/FlatLang/System": "master",
        "https://github.com/FlatLang/IO": "master",
        "https://github.com/FlatLang/Stream": "master"
      }
    },
    "test": {
      "mainClass": "flat/sendsafely/SendSafely_TestSuite",
      "source": "src/**/*_Test*.flat",
      "dependencies": {
        "https://github.com/FlatLang/Test": "master"
      }
    },
    "examples/CreatePackage": {
      "mainClass": "CreatePackage",
      "source": "examples",
      "outFile": "create-package.js",
      "dependencies": {}
    }
  }
}
